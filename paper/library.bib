issn = {23317019},
journal = {Physical Review Applied},
keywords = {doi:10.1103/PhysRevApplied.15.034068 url:https://d},
number = {3},
pages = {1},
publisher = {American Physical Society},
title = {{Variational Circuit Compiler for Quantum Error Correction}},
url = {https://doi.org/10.1103/PhysRevApplied.15.034068},
volume = {15},
year = {2021}
}
