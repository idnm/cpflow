file = {:home/idnm/Dropbox/hep/Quantum Computing/1508.03273.pdf:pdf},
month = {aug},
pages = {1--14},
title = {{On the advantages of using relative phase Toffolis with an application to multiple control Toffoli optimization}},
url = {http://arxiv.org/abs/1508.03273 http://dx.doi.org/10.1103/PhysRevA.93.022311},
year = {2015}
