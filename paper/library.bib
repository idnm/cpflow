issn = {17425468},
journal = {Journal of Statistical Mechanics: Theory and Experiment},
keywords = {Stationary states},
number = {4},
title = {{On the uniqueness of the steady-state solution of the Lindblad-Gorini-Kossakowski-Sudarshan equation}},
volume = {2019},
year = {2019}
}
