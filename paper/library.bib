eprint = {9805275},
file = {:home/idnm/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Von Delft, Schneller - 1998 - Bosonization for beginners - Refermionization for experts.pdf:pdf},
issn = {00033804},
